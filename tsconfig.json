{
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "target": "ESNext",
    "moduleResolution": "node",
  },
  "exclude": ["node_modules"],
}
